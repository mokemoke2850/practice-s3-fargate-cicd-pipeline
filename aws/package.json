    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.64.0",
    "cdk-docker-image-deployment": "^0.0.163",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
