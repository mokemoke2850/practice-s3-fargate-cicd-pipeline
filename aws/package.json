    "aws": "bin/aws.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "rm-image": "sh ./ecr-image-delete.sh ap-northeast-1 github-actions-test && sh ./ecr-image-delete.sh ap-northeast-1 cdk-hnb659fds-container-assets-284968725079-ap-northeast-1",
